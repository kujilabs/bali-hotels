<section id='side_body_content'> 
  <div id='gallery'>
    <% @page.images.each do |i| %>
      <%= image_tag(i.thumbnail("269x179#c").url) %>
    <% end %>
  </div>

  <%=raw @page.content_for(:side_body) %>
</section>

<section id='body_content'>
  <h1 id='logo' class="tk-dejarip"><%= @page.title %></h1>     
  <%=raw @page.content_for(:body) %>
</section>


